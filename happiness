#include<stdio.h>
#include<stdlib.h>
int top=-1;
int *stk;
int n,k;
void push(int ele)
{
 
 stk[++top]=ele;
}
int pop()
{
 int x;
 x=stk[top--];
 return x;
}
int main()
{
int i,len,count,max,flag=0,l;
int arr[100];
scanf("%d %d",&n,&k);
int check=k;
stk=(int*)malloc(n*sizeof(int));
top=n-1;
for(i=n-1;i>=0;i--)
{
  scanf("%d",&stk[i]);
}
count=0;
while(k!=1)
{
   arr[count++]=pop();
   k--;
}
max=stk[top];
for(i=0;i<count;i++)
{
 if(max<arr[i])
{
    flag=1;
  max=arr[i];
}
}
if(flag==1)
{
    push(max);
}
else
{
    l=pop();
}
 
 
printf("%d",stk[top]);
 
return 0;
}
